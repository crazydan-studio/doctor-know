    "build:docs": "",
    "test": "npm run unit && npm run e2e",
    "test:e2e": "karma start build/karma.e2e.config.js",
    "test:unit": "karma start build/karma.unit.config.js",
    "test:cover": "",
    "lint": "eslint .",
    "unit": "karma start build/karma.unit.config.js",
    "link": "npm run link:local-modules",
    "link:local-modules": "npm run link:doctor-know",
    "link:doctor-know": "node build/link-module --path=../src --name=doctor-know"
  },
